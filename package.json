		"husky": "^9.0.11",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^5.0.0-next.0",
		"svelte-check": "^3.6.0",
		"sveltekit-superforms": "^2.14.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^7.9.0",
		"vite": ">=5.0.13",
		"vitest": "^1.2.0",
